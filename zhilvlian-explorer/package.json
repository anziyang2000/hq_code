{
  "name": "exp",
  "version": "1.0.0",
  "private": true,
  "description": "exp",
  "scripts": {
    "build": "max build",
    "build:dev": "cross-env UMI_ENV=devbuild max build",
    "build:test": "cross-env UMI_ENV=testbuild max build",
    "build:canary": "cross-env UMI_ENV=canarybuild max build",
    "build:master": "cross-env UMI_ENV=prodbuild max build",
    "dev": "npm run start:dev",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "npm run start:dev",
    "start:dev": "cross-env REACT_APP_ENV=dev UMI_ENV=local max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=local max dev",
    "start:canary": "cross-env REACT_APP_ENV=canary MOCK=none UMI_ENV=local max dev",
    "start:prod": "cross-env REACT_APP_ENV=prod MOCK=none UMI_ENV=local max dev",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-components": "^2.0.0",
    "@umijs/route-utils": "^2.1.3",
    "antd": "^4.20.0",
    "classnames": "^2.3.0",
    "dayjs": "^1.11.6",
    "lodash": "^4.17.0",
    "omit.js": "^2.0.2",
    "rc-menu": "^9.1.0",
    "rc-tween-one": "^3.0.6",
    "rc-util": "^5.16.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-json-tree": "^0.17.0",
    "react-vant": "^3.0.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.0",
    "@types/classnames": "^2.3.1",
    "@types/history": "^4.7.0",
    "@types/lodash": "^4.14.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/fabric": "^2.11.1",
    "@umijs/max": "^4.2.5",
    "cross-env": "^7.0.0",
    "detect-installer": "^1.0.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.4",
    "lint-staged": "^10.0.0",
    "prettier": "^2.5.0",
    "typescript": "^4.5.0",
    "umi-presets-pro": "^1.0.1"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
